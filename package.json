{
  "name": "ig-mini-project",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./dist/index.js",
    "tsc": "tsc",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAsset",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "postinstall": "npm-run-all clean tslint tsc copy-assets",
    "dev": "NODE_ENV=development npm-run-all postinstall start",
    "start:dev": "nodemon --watch ./ -e ts,pug,css,js --exec npm run dev"
  },
  "engines": {
    "node": "11.14.0"
  },
  "author": "Daniel Charles Mwangila",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.20.1",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.18.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.0",
    "@types/cheerio": "^0.22.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.6",
    "@types/shelljs": "^0.8.6",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.8.2"
  },
  "nodemonConfig": {
    "ignore": "dist/*"
  }
}
